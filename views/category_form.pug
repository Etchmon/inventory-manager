extends layout 

block content 
    h1 #{title}

    form(method='POST' action='')
        .form-group
        label(for='type') Category:
        input#type.form-control(type='text', placeholder='Food Group' value=(undefined===category ? '': category.type) name='type')
        button.btn.btn-primary(type='submit') Submit

    if errors
        ul 
            for error in errors 
                li!= error.msg